{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base","eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "always"]
    }
}
